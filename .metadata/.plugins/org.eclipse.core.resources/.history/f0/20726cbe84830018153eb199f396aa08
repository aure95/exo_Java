package fr.exemple;

public class Run {

	public static void main(String[] args) {
		// fabriquer une instance de personne
		// c est aussi une variable locale de ma m√©thode main
		
		Personne p1=new Personne();
		Personne p2=new Personne();
		
		p1.setAge(25);
		p1.setNom("ghiojg");
		p1.setPrenom("flgpk");
		p1.setTaille(2.3D);
		
		p1.setAge(41);
		p1.setNom("hkjoh");
		p1.setPrenom("ghdfuhgu");
		p1.setTaille(0.9);
		
		
		System.out.println(p1.getAge());
		System.out.println(p2.getAge());

	}

}
