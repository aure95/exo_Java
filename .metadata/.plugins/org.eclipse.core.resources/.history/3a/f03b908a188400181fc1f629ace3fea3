import fr.dessin.Point2D;

public class Run {

	public static void main(String[] args) {
		
		Point2D p1=new Point2D();
		Point2D p2=new Point2D(1,2);
		
		p1.afficher();
		p2.afficher();
		
		p1.setX(1);
		p1.setY(2);
		
		p2.setX(4);
		p2.setY(5);
		
		p1.afficher();
		p2.afficher();
		
		p1.translater(2,2);
		p2.translater(0,1);
		
		p1.afficher();
		p2.afficher();
		
		System.out.println("compteur "+p1.getCompteur());
		
		
		
		
		
		

	}

}
