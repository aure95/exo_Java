package fr.exemple;

public class HelloWorld {

	public static void main(String[] args) {
		
		System.out.println("hello world");
		
		int monEntier=1;
		short monShort=2;
		byte monByte=3;
		
		//Float
		double monDouble=3.5D;
		double myDouble=3.5;
		
		float monFloat=6.3F;
		float myFloat=(float)6.3;
		
		//Boolean
		boolean monBoolean=true //ou false;
				
		
		
		

	}

}
