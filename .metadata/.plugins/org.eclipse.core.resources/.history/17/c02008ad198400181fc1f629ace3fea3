package fr.dessin;

public class Point3D extends Point2D {
	
	private int z;
	
	////////////////////////
	
	public Point3D() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Point3D(int vX, int vY) {
		super(vX, vY);
		// TODO Auto-generated constructor stub
	}
	
	///////////////////////
	
	public int getZ() {
		return z;
	}

	public void setZ(int z) {
		this.z = z;
	}

	
	
	//////////////////////////
	
	
	

}
